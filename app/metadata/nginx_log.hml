---
kind: ObjectType
version: v1
definition:
  name: NginxLog
  fields:
    - name: body_bytes_sent
      type: String!
    - name: container_name
      type: Label!
    - name: http_referer
      type: String!
    - name: http_user_agent
      type: String!
    - name: log_line
      type: LogLine!
      description: A log line if the flat values setting is enabled
    - name: log_lines
      type: "[StreamValue!]!"
      description: List of log lines grouped by unique labels
    - name: original_labels
      type: LabelSet!
      description: Labels of the metric
    - name: remote_addr
      type: String!
    - name: remote_user
      type: String!
    - name: request
      type: String!
    - name: request_length
      type: String!
    - name: request_time
      type: String!
    - name: status
      type: String!
    - name: time_local
      type: String!
    - name: timestamp
      type: Timestamp!
      description: The timestamp of the current log line or the last timestamp of a
        range query result
  graphql:
    typeName: NginxLog
    inputTypeName: NginxLog_input
  dataConnectorTypeMapping:
    - dataConnectorName: myloki
      dataConnectorObjectType: NginxLog

---
kind: TypePermissions
version: v1
definition:
  typeName: NginxLog
  permissions:
    - role: admin
      output:
        allowedFields:
          - body_bytes_sent
          - container_name
          - http_referer
          - http_user_agent
          - log_line
          - log_lines
          - original_labels
          - remote_addr
          - remote_user
          - request
          - request_length
          - request_time
          - status
          - time_local
          - timestamp

