        - fieldName: remote_addr
          booleanExpressionType: String_bool_exp
        - fieldName: remote_user
          booleanExpressionType: String_bool_exp
        - fieldName: request
          booleanExpressionType: String_bool_exp
        - fieldName: request_length
          booleanExpressionType: String_bool_exp
        - fieldName: request_time
          booleanExpressionType: String_bool_exp
        - fieldName: status
