      type: NginxLog
      comparableFields:
        - fieldName: body_bytes_sent
          booleanExpressionType: String_bool_exp
        - fieldName: container_name
          booleanExpressionType: Label_bool_exp
        - fieldName: http_referer
          booleanExpressionType: String_bool_exp
        - fieldName: http_user_agent
          booleanExpressionType: String_bool_exp
        - fieldName: log_line
