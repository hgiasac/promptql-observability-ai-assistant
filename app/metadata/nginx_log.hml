  graphql:
    typeName: NginxLog_bool_exp

---
kind: Model
version: v1
definition:
  name: nginx_log
  objectType: NginxLog
  arguments:
    - name: flat
      type: Boolean
      description: Flatten grouped values out the root array
    - name: interval
      type: Duration
      description: Only return entries at (or greater than) the specified interval,
        can be a duration format or float number of seconds
  source:
    dataConnectorName: myloki
    collection: nginx_log
  filterExpressionType: NginxLog_bool_exp
  orderableFields:
    - fieldName: body_bytes_sent
      orderByDirections:
        enableAll: true
    - fieldName: container_name
      orderByDirections:
        enableAll: true
    - fieldName: http_referer
      orderByDirections:
        enableAll: true
    - fieldName: http_user_agent
      orderByDirections:
        enableAll: true
    - fieldName: log_line
      orderByDirections:
        enableAll: true
    - fieldName: original_labels
      orderByDirections:
        enableAll: true
    - fieldName: remote_addr
      orderByDirections:
        enableAll: true
    - fieldName: remote_user
      orderByDirections:
        enableAll: true
    - fieldName: request
      orderByDirections:
        enableAll: true
    - fieldName: request_length
      orderByDirections:
        enableAll: true
    - fieldName: request_time
      orderByDirections:
        enableAll: true
    - fieldName: status
      orderByDirections:
        enableAll: true
    - fieldName: time_local
      orderByDirections:
        enableAll: true
    - fieldName: timestamp
      orderByDirections:
        enableAll: true
  graphql:
    selectMany:
      queryRootField: nginx_log
    selectUniques: []
    argumentsInputType: nginx_log_arguments
    orderByExpressionType: nginx_log_order_by

