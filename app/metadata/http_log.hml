---
kind: ObjectType
version: v1
definition:
  name: HttpLog
  fields:
    - name: container_name
      type: Label!
    - name: http_status
      type: String!
    - name: latency
      type: String!
    - name: level
      type: String!
    - name: log_line
      type: LogLine!
      description: A log line if the flat values setting is enabled
    - name: log_lines
      type: "[StreamValue!]!"
      description: List of log lines grouped by unique labels
    - name: operation_name
      type: String!
    - name: original_labels
      type: LabelSet!
      description: Labels of the metric
    - name: request_url
      type: String!
    - name: timestamp
      type: Timestamp!
      description: The timestamp of the current log line or the last timestamp of a
        range query result
    - name: type
      type: String!
  graphql:
    typeName: HttpLog
    inputTypeName: HttpLog_input
  dataConnectorTypeMapping:
